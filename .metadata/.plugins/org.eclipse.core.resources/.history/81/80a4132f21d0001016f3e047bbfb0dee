package arrays;

public class MaxElementInArray {
	
	
	           public int MaximumElement(int num[]){
	        	   int max = num[0];
	        	   
	        	   for(int i=1; i<num.length; i++) {
	        		   if(max<num[i]) {
	        			   max = num[i];
	        		   }
	        		  
	        	   }
	        	   return max;
	        	  
	        	   
	           }
              public static void main(String[] args) {
				MaximumElement m1 = new MaximumElement();
			}
}
